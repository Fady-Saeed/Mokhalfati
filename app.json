{
  "expo": {
    "name": "Mokhalfati",
    "slug": "mokhalfati",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/MokhalfatiLOGO.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mokhalfati.app",
      "icon": "./assets/images/MokhalfatiLOGO.png"
    },
    "android": {
      "package": "com.mokhalfati.app",
      "versionCode": 1,
      "icon": "./assets/images/MokhalfatiLOGO.png",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "CAMERA"
      ]
    },
    "description": "Mokhalfati is a mobile app for getting the cost of fine applied for a car easily with just a pic of the car license."
  }
}